<!-- 실험으로 location2에 넣는 중 -->
  	<insert id="insert" parameterType="Location2DTO">
 	<selectKey resultType="Integer" order="BEFORE" keyProperty="locationSeq">
			select location_seq.nextval from dual
	</selectKey>
 		insert into location(
		locationSeq,
		placeUrl,
		categoryCode,
		newAddress,
		title,
		locid,
		phone,
		address,
		longitude,
		latitude
		) values(#{locationSeq},
		#{place_url},
		#{category_group_code},
		#{road_address_name},
		#{place_name},
		#{id},
		#{phone},
		#{address_name},
		#{x},
		#{y})
 	</insert>  	
		
 </mapper>